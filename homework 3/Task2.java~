import java.util.*;
public class Task2{
  public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter Marks");
    int num=sc.nextInt();
    if(num>=90 && num<=100) System.out.println("A");
    if(num>=80 && num<=89) System.out.println("B");
    if(num>=70 && num<=79) System.out.println("C");
    if(num>=60 && num<=69) System.out.println("D");
    if(num>=50 && num<=59) System.out.println("E");
    if(num<=0 || num>=100) System.out.println("Wrong Input");
    if(num<50) System.out.println("F");
    sc.close();
  }
}
    